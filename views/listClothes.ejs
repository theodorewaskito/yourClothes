<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
</head>
<body>
  <h1>List Clothes</h1>
  <a href="/clothes/add">Add Clothes</a><br><br>
  <table border="1">
    <tr>
      <th>Name</th>
      <th>Material</th>
      <th>Color</th>
      <th>Price</th>
      <th>Stock</th>
      <th>Image</th>
      <th>Action</th>
    </tr>
    <% data.forEach( el => { %>
    <tr>
        <td><%= el.name %></td>
        <td><%= el.material %></td>
        <td><%= el.color %></td>
        <td><%= el.price %></td>
        <td><%= el.stock %></td>
        <td><img src="<%= el.image %>" alt="#"></td>
        <td><a href="#">Edit</a> | <a href="clothes/delete/<%= el.id %>">Delete</a></td>
    </tr>
    <% }) %>
  </table>
</body>
</html>